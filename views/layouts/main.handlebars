<body>
  <nav class="navbar justified bg-info">
    <div class="">
      <div class="float-left">
        <a class="navbar-brand text-light p-3" href="/">
          <h1 class="m-2"><i class="fa fa-microchip"></i>Tech Blog
          </h1>
        </a>
      </div>
    </div>
    <div class="">
      <div class="content-center float-right">
        {{#if firstName}}
        <H3 class="text-light fst-italic text-center">Welcome {{firstName}} </H3>
        {{/if}}
        <a id="home" class="navbar-brand text-light p-3 " href="/"><i class="m-1 fas fa-home"></i>Home</a>
        <a id="dashboard" class="navbar-brand text-light p-3 " href="/dashboard"><i
            class="m-1 fas fa-dashboard"></i>Dashboard</a>
        {{#if loggedIn}}
        <a id="logout" class="navbar-brand text-light p-3" href="/"><i
            class="m-1 fas fa-right-from-bracket"></i></i>Sign Out</a>
        {{ else }}
        <a href="" id="login" class="navbar-brand text-light p-3" data-bs-toggle="modal" data-bs-target="#loginModal"><i
            class="m-1 fas fa-right-to-bracket"></i></i>Sign In</a>
        {{/if}}
      </div>
    </div>

  </nav>
  {{{ body }}}
  {{> login-modal}}

  {{#if loggedIn}}
  <script src="/js/logout.js"></script>
  {{else}}
  <script src="/js/login.js"></script>
  {{/if}}

  <link rel="stylesheet" href="/css/globals.css">
